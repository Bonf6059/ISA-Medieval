<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- Replace Chemfuel Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Chemfuel"]/graphicData/texPath</xpath>
    <value>
      <texPath>Items/Resources/Misc/TribalChemfuel/TribalChemfuel_a</texPath>
    </value>
  </Operation>

<!-- Replace Vanilla beds Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Bed"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/Beds/Vanilla Bed/BedWicker</texPath>
    </value>
  </Operation>

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="DoubleBed"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/Beds/Vanilla Bed/BedWickerDouble</texPath>
    </value>
  </Operation>

    <!-- Replace Vanilla dresser Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Dresser"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/Dresser/RusticDresser</texPath>
    </value>
  </Operation>

    <!-- Replace Vanilla end table Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="EndTable"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/EndTable/RusticEndTable</texPath>
    </value>
  </Operation>

    <!-- Replace Vanilla stool Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Stool"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/StoolChair/RusticRoundStool</texPath>
    </value>
  </Operation>

    <!-- Replace Vanilla Dining Chair Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="DiningChair"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/StoolChair/RusticChair</texPath>
    </value>
  </Operation>

    <!-- Replace Vanilla 1x2 table Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Table1x2c"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/Table/ReinforcedRusticTable1x2</texPath>
    </value>
  </Operation>
  
     <!-- Replace Mead Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="VFEM2_Mead"]/graphicData/texPath</xpath>
    <value>
      <texPath>Items/Drinks/Mead</texPath>
    </value>
  </Operation>

     <!-- Replace Fermenting Barrel framework texture -->
  
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="BarrelProcessor"]/graphicData</xpath>
		<value>
			<graphicData>
				<texPath>Production/RusticFermentingBarrel/RusticFermentingBarrel</texPath>
				<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>2,2</drawSize>
				<damageData>
					<rect>(0.05,0.1,0.9,0.9)</rect>
				</damageData>
				<shadowData>
					<volume>(0.75, 0.35, 0.33)</volume>
				</shadowData>
			</graphicData>
		</value>
	</Operation>
   
</Patch>
