<?xml version="1.0" encoding="utf-8"?>
<Patch>

<!-- Replace Chemfuel Texture -->
	
  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Chemfuel"]/graphicData</xpath>
    <value>
		<graphicData>
    		<texPath>Items/Resources/Misc/TribalChemfuel</texPath>
    		<graphicClass>Graphic_StackCount</graphicClass>
			<drawSize>0.85</drawSize>
    	</graphicData>
    </value>
  </Operation>

<!-- Replace Beer Texture -->
	
  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Beer"]/graphicData/texPath</xpath>
    <value>
    	<texPath>Items/Drinks/Ale</texPath>
    </value>
  </Operation>
	
<!-- Replace Vanilla beds Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Bed"]/graphicData</xpath>
    <value>
		<graphicData>
      		<texPath>Building/Furnitures/Beds/RusticBed/Green/RusticBed1x2_Green</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3,3)</drawSize>
			<damageData>
				<rect>(0,0.05,1,1.95)</rect>
			</damageData>
		</graphicData>
    </value>
  </Operation>
	
  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="DoubleBed"]/graphicData</xpath>
    <value>
      <graphicData>
			<texPath>Building/Furnitures/Beds/RusticBed/Green/RusticBed2x2_Green</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(4.1,4.1)</drawSize>
			<damageData>
				<rect>(0,0.05,2,1.95)</rect>
			</damageData>
		</graphicData>
    </value>
  </Operation>
	
    <!-- Replace Vanilla dresser Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Dresser"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/Dresser/RusticDresser</texPath>
    </value>
  </Operation>

    <!-- Replace Vanilla end table Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="EndTable"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/EndTable/RusticEndTable</texPath>
    </value>
  </Operation>

    <!-- Replace Vanilla stool Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Stool"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/StoolChair/RusticRoundStool</texPath>
    </value>
  </Operation>

    <!-- Replace Vanilla Dining Chair Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="DiningChair"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/StoolChair/RusticChair</texPath>
    </value>
  </Operation>

    <!-- Replace Vanilla 1x2 table Texture -->

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Table1x2c"]/graphicData/texPath</xpath>
    <value>
      <texPath>Building/Furnitures/Table/ReinforcedRusticTable1x2</texPath>
    </value>
  </Operation>

     <!-- Replace Fermenting Barrel framework texture -->
  
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="BarrelProcessor"]/graphicData</xpath>
		<value>
			<graphicData>
				<texPath>Building/Production/RusticFermentingBarrel/RusticFermentingBarrel</texPath>
				<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>2,2</drawSize>
				<damageData>
					<rect>(0.05,0.1,0.9,0.9)</rect>
				</damageData>
				<shadowData>
					<volume>(0.75, 0.35, 0.33)</volume>
				</shadowData>
			</graphicData>
		</value>
	</Operation>

    <!-- Replace Vanilla Tailor Bench texture -->
    <Operation Class="PatchOperationReplace">
        <xpath>Defs/ThingDef[defName="HandTailoringBench"]/graphicData/texPath</xpath>
        <value>
            <texPath>Building/Production/TailorsBench/TailorsBench</texPath>
        </value>
    </Operation>

    <!-- Replace Vanilla Brewery Table texture -->
    <Operation Class="PatchOperationReplace">
        <xpath>Defs/ThingDef[defName="Brewery"]/graphicData/texPath</xpath>
        <value>
            <texPath>Building/Production/Brewery/TableBreweryStuffed</texPath>
        </value>
    </Operation>

    <Operation Class="PatchOperationConditional">
        <xpath>Defs/ThingDef[defName="Brewery"]/graphicData/shaderType</xpath>
        <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="Brewery"]/graphicData</xpath>
            <value>
                <shaderType>CutoutComplex</shaderType>
            </value>
        </nomatch>
    </Operation>

    <!-- Replace Vanilla Stone cutting Table texture -->
    <Operation Class="PatchOperationReplace">
        <xpath>Defs/ThingDef[defName="TableStonecutter"]/graphicData/texPath</xpath>
        <value>
            <texPath>Building/Production/TableStonecutter/TableStonecutter</texPath>
        </value>
    </Operation>

    <Operation Class="PatchOperationConditional">
        <xpath>Defs/ThingDef[defName="TableStonecutter"]/graphicData/shaderType</xpath>
        <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThingDef[defName="TableStonecutter"]/graphicData</xpath>
            <value>
                <shaderType>CutoutComplex</shaderType>
            </value>
        </nomatch>
    </Operation>

    <!-- Replace Vanilla Research Bench texture -->
	
	<Operation Class="PatchOperationReplace">
        <xpath>Defs/ThingDef[defName="SimpleResearchBench"]/graphicData/texPath</xpath>
        <value>
            <texPath>Building/Production/ResearchBench/ResearchBenchSimple</texPath>
        </value>
    </Operation>

    <!-- Replace Vanilla Art Bench texture -->
	
	<Operation Class="PatchOperationReplace">
        <xpath>Defs/ThingDef[defName="TableSculpting"]/graphicData/texPath</xpath>
        <value>
            <texPath>Building/Production/ArtTable/TableSculpting</texPath>
        </value>
    </Operation>
	
</Patch>
