<?xml version="1.0" encoding="utf-8" ?>
<Defs>  

<!-- Uncooked Stew -->
  
	<ThingDef ParentName="MealBase">
		<defName>ISA_UncookedStew</defName>
		<label>uncooked stew</label>
		<description>An uncooked stew. This needs to be cooked in a stew pot to be edible.</description>
		<graphicData>
			<texPath>Food/Stew/Uncooked/ISA_UncookedStew</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingSetMakerTags></thingSetMakerTags>
		
		<statBases>
			<MarketValue>153.4</MarketValue>
			<Flammability>1.3</Flammability>
			<WorkToMake>2000</WorkToMake>

		</statBases>
		<tradeability>None</tradeability>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>4</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
			<li Class="CompProperties_Ingredients" />

		</comps>
		<ingestible>
			<preferability>NeverForNutrition</preferability>			
		</ingestible>
		
	</ThingDef>

<!-- Cooked Stew -->
  
	<ThingDef ParentName="MealCooked">
		<defName>ISA_CookedStew</defName>
		<label>Stew</label>
		<description>A stew slow cooked from a few ingredients.</description>
		<graphicData>
			<texPath>Food/Stew/Stew/Stew</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>24</MarketValue>
			<Nutrition>1</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealLavish</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Ingest_Drink</ingestSound>
		</ingestible>

	</ThingDef>

</Defs>  
