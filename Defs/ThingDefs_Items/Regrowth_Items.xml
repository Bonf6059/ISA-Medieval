	<!--====================  Mammoth Tusk ===========================-->
	
	<ThingDef ParentName="ResourceVerbBase">
		<defName>RG_MammothTusk</defName>
		<label>mammoth tusk</label>
		<description>The tusk of a mammoth. It can be sold to traders for silver.</description>
		<graphicData>
			<texPath>Things/Item/Special/MammothTusk</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<equippedAngleOffset>-20</equippedAngleOffset>
		<possessionCount>1</possessionCount>
		<statBases>
			<MarketValue>120</MarketValue>
			<MaxHitPoints>160</MaxHitPoints>
			<Mass>2</Mass>
			<Flammability>0.9</Flammability>
			<DeteriorationRate>0.5</DeteriorationRate>
		</statBases>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
		<stackLimit>10</stackLimit>
		<relicChance>0.5</relicChance>
		<weaponClasses>
			<li>Melee</li>
			<li>MeleePiercer</li>
		</weaponClasses>
		<thingCategories>
			<li>ItemsMisc</li>
		</thingCategories>
		<tradeTags>
			<li>ExoticMisc</li>
		</tradeTags>
		<thingSetMakerTags>
			<li>AnimalPart</li>
		</thingSetMakerTags>
		<tools>
			<li>
				<label>point</label>
				<capacities>
					<li>Scratch</li>
					<li>Stab</li>
				</capacities>
				<power>18.2</power>
				<cooldownTime>2.6</cooldownTime>
			</li>
			<li>
				<label>base</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>9</power>
				<cooldownTime>2</cooldownTime>
				<chanceFactor>0.5</chanceFactor>
			</li>
		</tools>
		<comps>
			<li Class="CompProperties_Styleable"/>
		</comps>
	</ThingDef>

		<!--====================  Dinosaurs Hides ===========================-->
	
		<!--====================  Thin ===========================-->
	
    <ThingDef ParentName="LeatherBase">
        <defName>RG_DinosaursHideThin</defName>
        <label>thin dinosaurs hide</label>
        <description>Lightweight, thin leather from a small theropod or sauropod. Doesn't insulate well.</description>
        <graphicData>
            <color>(183,175,79)</color>
        </graphicData>
        <statBases>
            <MarketValue>2.1</MarketValue>
            <StuffPower_Armor_Sharp>0.56</StuffPower_Armor_Sharp>
            <StuffPower_Armor_Blunt>0.18</StuffPower_Armor_Blunt>
            <StuffPower_Insulation_Cold>12</StuffPower_Insulation_Cold>
            <StuffPower_Insulation_Heat>12</StuffPower_Insulation_Heat>
        </statBases>
        <stuffProps>
            <color>(183,175,79)</color>
            <commonality>0.1</commonality>
            <statFactors>
                <MaxHitPoints>1.0</MaxHitPoints>
            </statFactors>
        </stuffProps>
    </ThingDef>

