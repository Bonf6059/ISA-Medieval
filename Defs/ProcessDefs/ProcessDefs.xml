<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<!-- Cheese -->
	<ProcessorFramework.ProcessDef Name="ISA_CheesePressBase" Abstract="True">
		<processDays>1</processDays><!-- in days, a day is 60,000 ticks -->
		<capacityFactor>1.0</capacityFactor>
		<efficiency>0.5</efficiency>
		<usesTemperature>true</usesTemperature>
		<temperatureSafe>-1.0~32.0</temperatureSafe>
		<temperatureIdeal>5.0~32.0</temperatureIdeal>
		<ruinedPerDegreePerHour>2</ruinedPerDegreePerHour><!-- percent per degree per hour -->
		<speedBelowSafe>0.1</speedBelowSafe>
		<speedAboveSafe>1.0</speedAboveSafe>
		<sunFactor>1.0~1.0</sunFactor>
		<rainFactor>1.0~1.0</rainFactor>
		<snowFactor>1.0~1.0</snowFactor>
		<windFactor>1.0~1.0</windFactor>
		<unpoweredFactor>0.0</unpoweredFactor>
		<unfueledFactor>0.0</unfueledFactor>
		<powerUseFactor>1.0</powerUseFactor>
		<fuelUseFactor>0.0</fuelUseFactor>
		<filledGraphicSuffix>Filled</filledGraphicSuffix>
		<usesQuality>true</usesQuality>
		<qualityDays>(1, 3, 8, 14, 20, 50, 120)</qualityDays>
		<color>(255, 255, 255)</color><!-- 0-255 or 0.0-1.0 -->
		<customLabel/>
		<destroyChance>0.0</destroyChance>
	</ProcessorFramework.ProcessDef>

	<ProcessorFramework.ProcessDef ParentName="ISA_CheesePressBase">
		<defName>ISA_MilkIntoCheese</defName>
		<thingDef>ISA_MealCheese</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>Milk</li>
			</thingDefs>
		</ingredientFilter>
	</ProcessorFramework.ProcessDef>

	<ProcessorFramework.ProcessDef MayRequire="Van.Beasts" ParentName="ISA_CheesePressBase">
		<defName>ISA_MufftonMilkIntoCheese</defName>
		<thingDef>ISA_MealMufftonCheese</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>DA_MufftonMilk</li>
			</thingDefs>
		</ingredientFilter>
	</ProcessorFramework.ProcessDef>
	
	<ProcessorFramework.ProcessDef MayRequire="Van.Beasts" ParentName="ISA_CheesePressBase">
		<defName>ISA_GoldiloxMilkIntoCheese</defName>
		<thingDef>ISA_MealGoldiloxCheese</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>DA_RegalMilk</li>
			</thingDefs>
		</ingredientFilter>
	</ProcessorFramework.ProcessDef>

	<!-- Grape Juice -->
	<ProcessorFramework.ProcessDef MayRequire="OskarPotocki.VFE.Medieval2">
		<defName>ISA_JuiceGrape</defName>
		<thingDef>ISA_JuiceGrape</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>VFEM2_Plant_Grape</li>
			</thingDefs>
		</ingredientFilter>
		<processDays>1</processDays><!-- in days, a day is 60,000 ticks -->
		<capacityFactor>1.0</capacityFactor>
		<efficiency>1.0</efficiency>
		<usesTemperature>true</usesTemperature>
		<temperatureSafe>-99.0~99.0</temperatureSafe>
		<temperatureIdeal>21.0~99.0</temperatureIdeal>
		<ruinedPerDegreePerHour>2.5</ruinedPerDegreePerHour><!-- percent per degree per hour -->
		<speedBelowSafe>0.1</speedBelowSafe>
		<speedAboveSafe>1.0</speedAboveSafe>
		<sunFactor>1.0~1.0</sunFactor>
		<rainFactor>1.0~1.0</rainFactor>
		<snowFactor>1.0~1.0</snowFactor>
		<windFactor>1.0~1.0</windFactor>
		<unpoweredFactor>0.0</unpoweredFactor>
		<unfueledFactor>0.0</unfueledFactor>
		<powerUseFactor>1.0</powerUseFactor>
		<fuelUseFactor>0.0</fuelUseFactor>
		<filledGraphicSuffix>Filled</filledGraphicSuffix>
		<usesQuality>false</usesQuality>
		<qualityDays>(1, 2, 3, 4, 5, 6, 7)</qualityDays>
		<color>(255, 255, 255)</color><!-- 0-255 or 0.0-1.0 -->
		<customLabel/>
		<destroyChance>0.0</destroyChance>
	</ProcessorFramework.ProcessDef>

</Defs>
