<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ProcessorFramework.ProcessDef ParentName="ISA_Processor_AlcoholBase">
		<defName>ISA_MustIntoWine</defName>
		<thingDef>ISA_Wine</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>ISA_MustWine</li>
			</thingDefs>
		</ingredientFilter>
		<processDays>6</processDays><!-- in days, a day is 60,000 ticks -->
		<capacityFactor>1.0</capacityFactor><!-- do not use too many decimals e.g 0.333 is fine for 1/3 -->
	</ProcessorFramework.ProcessDef>

	<ProcessorFramework.ProcessDef ParentName="ISA_Processor_AlcoholBase">
		<defName>ISA_MustIntoBerryWine</defName>
		<thingDef>ISA_Wine_Berry</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>ISA_MustWine_Berry</li>
			</thingDefs>
		</ingredientFilter>
		<processDays>6</processDays><!-- in days, a day is 60,000 ticks -->
		<capacityFactor>1.0</capacityFactor><!-- do not use too many decimals e.g 0.333 is fine for 1/3 -->
	</ProcessorFramework.ProcessDef>

	<ProcessorFramework.ProcessDef ParentName="ISA_Processor_AlcoholBase">
		<defName>ISA_AppleJuiceIntoCider</defName>
		<thingDef>ISA_Cider</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>ISA_JuiceApple</li>
			</thingDefs>
		</ingredientFilter>
		<processDays>3</processDays><!-- in days, a day is 60,000 ticks -->
		<capacityFactor>1.0</capacityFactor><!-- do not use too many decimals e.g 0.333 is fine for 1/3 -->
	</ProcessorFramework.ProcessDef>
	

</Defs>
