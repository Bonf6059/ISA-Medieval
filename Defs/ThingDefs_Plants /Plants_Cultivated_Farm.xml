	<!--=========================== Advanced Agriculture ==============================-->

	<!-- Grape -->
	
	<ThingDef ParentName="PlantBase">
		<defName>ISA_Plant_Grape</defName>
		<label>grape plant</label>
		<description>A medium-sized vine that yields grapes when harvested - an essential ingredient in the production of wine that also tastes nice when eaten raw. Takes a long time to initially grow but can be harvested repeatedly in a single lifespan.</description>
		<descriptionHyperlinks>
			<ThingDef>ISA_RawGrapes</ThingDef>
		</descriptionHyperlinks>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Plants/FullGrown/GrapePlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible/>
		<plant>
			<fertilitySensitivity>0.9</fertilitySensitivity>
			<growDays>19.5</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<harvestAfterGrowth>0.30</harvestAfterGrowth>
			<harvestedThingDef>ISA_RawGrapes</harvestedThingDef>
			<harvestYield>12</harvestYield>
			<sowMinSkill>5</sowMinSkill>
			<!-- <sowResearchPrerequisites>
				<li>DankPyon_AdvancedAgriculture</li>
			</sowResearchPrerequisites>	-->
			<sowTags>
				<li>Ground</li>
				<li>Hydroponics</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<wildOrder>2</wildOrder>
			<immatureGraphicPath>Plants/Immature/GrapePlant</immatureGraphicPath>
			<purpose>Food</purpose>
			<visualSizeRange>1~2</visualSizeRange>
		</plant>
	</ThingDef>

	<!-- Pumpkins -->
	
	<ThingDef ParentName="PlantBase">
		<defName>ISA_Plant_Pumpkins</defName>
		<label>pumpkin plant</label>
		<description>A thin green vine which gives way to a characteristically massive orange vegetable, a good visualization of "big things come in small packages". Not particularly tasty on its own, but it makes for a variety of hearty seasonal meals.</description>
		<descriptionHyperlinks>
			<ThingDef>ISA_RawPumpkins</ThingDef>
		</descriptionHyperlinks>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Nutrition>0.5</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Plants/FullGrown/PumpkinPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible/>
		<plant>
			<fertilityMin>0.7</fertilityMin>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>1</maxMeshCount>
			<harvestedThingDef>ISA_RawPumpkins</harvestedThingDef>
			<harvestYield>20</harvestYield>
			<!--	<sowResearchPrerequisites>
				<li>DankPyon_AdvancedAgriculture</li>
			</sowResearchPrerequisites>	-->
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<topWindExposure>0.20</topWindExposure>
			<growDays>8</growDays>
			<fertilitySensitivity>0.9</fertilitySensitivity>
			<visualSizeRange>0.8~1.0</visualSizeRange>
			<immatureGraphicPath>Plants/Immature/PumpkinPlant</immatureGraphicPath>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
			<sowMinSkill>8</sowMinSkill>
			<vacuumResistant>true</vacuumResistant>
		</plant>
	</ThingDef>

	<!--=========================== Fruit Trees ==============================-->

	<!-- Apple tree -->
	
	<ThingDef ParentName="TreeBase">
		<defName>ISA_Tree_Apple</defName>
		<label>apple tree</label>
		<description>A hardwood tree known for its wonderfully sweet, bright-red fruits, which are good both on their own and mixed into a variety of desserts.</description>
		<descriptionHyperlinks>
			<ThingDef>ISA_RawApples</ThingDef>
		</descriptionHyperlinks>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Nutrition>1.5</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Plants/FullGrown/TreeApple</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible/>
		<plant>
			<dieIfLeafless>False</dieIfLeafless>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<fertilitySensitivity>0.7</fertilitySensitivity>
			<growDays>33</growDays>
			<harvestTag>Standard</harvestTag>
			<harvestAfterGrowth>0.30</harvestAfterGrowth>
			<harvestedThingDef>ISA_RawApples</harvestedThingDef>
			<harvestYield>20</harvestYield>
			<sowMinSkill>3</sowMinSkill>
			<mustBeWildToSow>false</mustBeWildToSow>
			<sowResearchPrerequisites Inherit="False">
				<li>Cocoa</li>
			</sowResearchPrerequisites>
			<topWindExposure>0.1</topWindExposure>
			<wildOrder>2</wildOrder>
			<immatureGraphicPath>Plants/Immature/TreeApple</immatureGraphicPath>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

</Defs>
