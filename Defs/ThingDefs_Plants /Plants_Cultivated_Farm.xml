<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Garlic -->
		
	<ThingDef ParentName="PlantBase">
		<defName>ISA_Plant_Garlic</defName>
		<label>garlic plant</label>
		<description>Leafy green stalks, who's base consists of a segmented off-white bulb. Garlic makes for a good seasoning and ingredient for many meals, either minced and sauteed for use as an ingredient, or pulverized and used as a seasoning.</description>
		<descriptionHyperlinks>
			<ThingDef>ISA_RawGarlic</ThingDef>
		</descriptionHyperlinks>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Nutrition>0.15</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Plants/FullGrown/GarlicPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible/>
		<plant>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>9</maxMeshCount>
			<harvestedThingDef>ISA_RawGarlic</harvestedThingDef>
			<harvestYield>6</harvestYield>
		<!--	<sowResearchPrerequisites>
				<li>DankPyon_BasicAgriculture</li>
			</sowResearchPrerequisites>	-->
			<sowTags>
				<li>Ground</li>
				<li>Hydroponics</li>
			</sowTags>
			<topWindExposure>0.20</topWindExposure>
			<growDays>4</growDays>
			<fertilitySensitivity>0.2</fertilitySensitivity>
			<visualSizeRange>0.2~0.4</visualSizeRange>
			<immatureGraphicPath>Plants/Immature/GarlicPlant</immatureGraphicPath>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
			<sowMinSkill>3</sowMinSkill>
		</plant>
	</ThingDef>

	<!-- Carrots -->
	
	<ThingDef ParentName="PlantBase">
		<defName>ISA_Plant_Carrots</defName>
		<label>carrot plant</label>
		<description>Bushy, short stalks whos value is in the sugary taproot. They make for a healthy, if not tasty snacks on their own, or a good addition to any meal when boiled, stewed or sauteed.</description>
		<descriptionHyperlinks>
			<ThingDef>ISA_RawCarrots</ThingDef>
		</descriptionHyperlinks>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Plants/FullGrown/CarrotPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible/>
		<plant>
			<!--fertilityMin>0.6</fertilityMin-->
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>4</maxMeshCount>
			<harvestedThingDef>ISA_RawCarrots</harvestedThingDef>
			<harvestYield>6</harvestYield>
		<!--	<sowResearchPrerequisites>
				<li>DankPyon_IntermediateAgriculture</li>
			</sowResearchPrerequisites>	-->
			<sowTags>
				<li>Ground</li>
				<li>Hydroponics</li>
			</sowTags>
			<topWindExposure>0.20</topWindExposure>
			<growDays>3.25</growDays>
			<fertilitySensitivity>0.6</fertilitySensitivity>
			<visualSizeRange>0.2~0.4</visualSizeRange>
			<immatureGraphicPath>Plants/Immature/CarrotPlant</immatureGraphicPath>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
			<sowMinSkill>4</sowMinSkill>
		</plant>
	</ThingDef>

	<!-- Herbs -->
	
	<ThingDef ParentName="PlantBase">
		<defName>ISA_Plant_Herb</defName>
		<label>herb plant</label>
		<description>A fragrant, leafy green plant which has an iota of uses in food preparation as a flavor enhancer, but which is virtually inedible on its own.</description>
		<descriptionHyperlinks>
			<ThingDef>ISA_RawHerb</ThingDef>
		</descriptionHyperlinks>
		<statBases>
			<MaxHitPoints>65</MaxHitPoints>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Plants/FullGrown/HerbPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible/>
		<plant>
			<!--fertilityMin>0.70</fertilityMin
			<sowResearchPrerequisites>
				<li>DankPyon_IntermediateAgriculture</li>
			</sowResearchPrerequisites>	-->
			<fertilitySensitivity>0.50</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<maxMeshCount>4</maxMeshCount>
			<harvestedThingDef>ISA_RawHerb</harvestedThingDef>
			<harvestYield>2</harvestYield>
			<harvestWork>200</harvestWork>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<growDays>5.6</growDays>
			<visualSizeRange>0.2~0.4</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<!-- Tomatoes -->
	
	<ThingDef ParentName="PlantBase">
		<defName>ISA_Plant_Tomatoes</defName>
		<label>tomato plant</label>
		<description>A thick, leafy green vine which gives birth to many large bulbous red fruits. While weither it exists as a fruit or a vegetable is a hotly debated topic, what is known is that the tomatoes it creates are juicy and delicious.</description>
		<descriptionHyperlinks>
			<ThingDef>ISA_RawTomatoes</ThingDef>
		</descriptionHyperlinks>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Plants/FullGrown/TomatoPlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible/>
		<plant>
			<fertilitySensitivity>1</fertilitySensitivity>
			<growDays>4.25</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<harvestAfterGrowth>0.30</harvestAfterGrowth>
			<harvestedThingDef>ISA_RawTomatoes</harvestedThingDef>
			<harvestYield>8</harvestYield>
			<sowMinSkill>4</sowMinSkill>
			<!-- <sowResearchPrerequisites>
				<li>DankPyon_IntermediateAgriculture</li>
			</sowResearchPrerequisites>	-->
			<sowTags>
				<li>Ground</li>
				<li>Hydroponics</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<wildOrder>2</wildOrder>
			<visualSizeRange>1~2.0</visualSizeRange>
			<immatureGraphicPath>Plants/Immature/TomatoPlant</immatureGraphicPath>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<!-- Cabbages -->

	<ThingDef ParentName="PlantBase">
		<defName>ISA_Plant_Cabbages</defName>
		<label>cabbage plant</label>
		<description>Layered leafy vegetable for its cold resilience. Cabbage is especially good as an ingredient in salads and soups.</description>
		<descriptionHyperlinks>
			<ThingDef>ISA_RawCabbages</ThingDef>
		</descriptionHyperlinks>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.4</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Plants/FullGrown/CabbagePlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible/>
		<plant>
			<fertilitySensitivity>0.9</fertilitySensitivity>
			<growDays>8</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>ISA_RawCabbages</harvestedThingDef>
			<harvestYield>10</harvestYield>
			<sowMinSkill>4</sowMinSkill>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
			<visualSizeRange>0.7~0.7</visualSizeRange>
			<minGrowthTemperature>-18</minGrowthTemperature>
			<minOptimalGrowthTemperature>0</minOptimalGrowthTemperature>
			<minOptimalGrowthTemperature>25</minOptimalGrowthTemperature>
			<maxGrowthTemperature>35</maxGrowthTemperature>
		</plant>
	</ThingDef>

	<!-- Wheat -->

	<ThingDef ParentName="PlantBase">
		<defName>ISA_Plant_Wheat</defName>
		<label>wheat plant</label>
		<description>A tall, slow-growing crop that has a high yield of small yellow grains. These grains can be easily turned into flour with the slightest touch. Needs fertile soil to grow well.</description>
		<descriptionHyperlinks>
			<ThingDef>ISA_Flour</ThingDef>
		<!--	<ThingDef>Hay</ThingDef>	-->
		</descriptionHyperlinks>
		<statBases>
			<MaxHitPoints>80</MaxHitPoints>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Plants/FullGrown/PlantWheat</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
