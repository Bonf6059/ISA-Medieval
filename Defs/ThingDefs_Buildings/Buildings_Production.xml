<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<!--1x3 Bench Base-->
	<ThingDef ParentName="BenchBase" Name="ISA_1X3WorkbenchBase" Abstract="True">
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.5</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<WorkToBuild>2000</WorkToBuild>
			<MaxHitPoints>180</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<size>(3,1)</size>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<techLevel>Medieval</techLevel>
		<placeWorkers>
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
	</ThingDef>
  
<!-- Researching spot from medieval overhaul -->
  
	<ThingDef ParentName="BuildingBase">
		<defName>ISA_ResearchingSpot</defName>
		<label>researching spot</label>
		<description>A place for research simple concepts. Researching here is slow because of the lack of tools.</description>
		<thingClass>Building_ResearchBench</thingClass>
		<graphicData>
			<texPath>Things/Building/Production/CraftingSpot</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1,1)</drawSize>
		</graphicData>
		<altitudeLayer>FloorEmplacement</altitudeLayer>
		<statBases>
			<WorkToBuild>0</WorkToBuild>
			<ResearchSpeedFactor>0.5</ResearchSpeedFactor>
		</statBases>
		<scatterableOnMapGen>false</scatterableOnMapGen>
		<useHitPoints>false</useHitPoints>
		<size>(1,1)</size>
		<designationCategory>Production</designationCategory>
		<passability>Standable</passability>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<sowTag>SupportPlantsOnly</sowTag>
			<canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
			<ai_chillDestination>false</ai_chillDestination>
			<artificialForMeditationPurposes>false</artificialForMeditationPurposes>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
		<placeWorkers>
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
	</ThingDef>

<!-- Butcher's block from medieval overhaul -->

	  <ThingDef ParentName="BenchBase">
		<defName>ISA_ButchersBlock</defName>
		<label>butcher's block</label>
		<description>A wooden stump for butchering dead creatures into pieces of raw meat.</description>
  		<thingClass>Building_WorkTable</thingClass>
		<graphicData>
			<texPath>Production/ButcherBlock/ButcherBlock</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(1.5,1.5)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<costStuffCount>50</costStuffCount>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.2</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>2000</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <Cleanliness>-15</Cleanliness>
    </statBases>
    <size>(1,1)</size>
    <designationCategory>Production</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <!--inherit recipes for mod compatibility-->
  		<modExtensions>
			<li Class="VEF.Buildings.RecipeInheritanceExtension">
				<inheritRecipesFrom>
					<li>TableButcher</li>
				</inheritRecipesFrom>
     			</li>
		</modExtensions>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
      <workTableRoomRole />
    </building>
    <comps>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>ToolCabinet</li>
        </linkableFacilities>
      </li>
    </comps>
    <designationHotKey>Misc3</designationHotKey>
    <placeWorkers>
      <li>PlaceWorker_ShowFacilitiesConnections</li>
      <li>PlaceWorker_PreventInteractionSpotOverlap</li>
    </placeWorkers>
		<techLevel>Medieval</techLevel>
  </ThingDef>

<!-- Rustic cooking table from medieval overhaul -->
  
	<ThingDef ParentName="ISA_1X3WorkbenchBase">
		<defName>ISA_RusticCookingTable</defName>
		<label>rustic cooking table</label>
		<description>A wood-fueled oven with an attached countertop for preparing meals.</description>
		<thingClass>Building_WorkTable_HeatPush</thingClass>
		<graphicData>
			<texPath>Production/RusticCookingTable/RusticCookingTable</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3.5,1.5)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<castEdgeShadows>true</castEdgeShadows>
		<staticSunShadowHeight>0.20</staticSunShadowHeight>
		<costList>
			<WoodLog>60</WoodLog>
		</costList>
		<costStuffCount>20</costStuffCount>
		<stuffCategories>
			<li>Stony</li>
		</stuffCategories>
    <!--inherit recipes for mod compatibility-->
  		<modExtensions>
			<li Class="VEF.Buildings.RecipeInheritanceExtension">
				<inheritRecipesFrom>
					<li>FueledStove</li>
				</inheritRecipesFrom>
     			</li>
		</modExtensions>
		<comps>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>160.0</fuelConsumptionRate>
				<fuelCapacity>50.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
				</fuelFilter>
				<consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
			</li>
			<li Class="CompProperties_HeatPusher">
				<compClass>CompHeatPusherPowered</compClass>
				<heatPerSecond>4</heatPerSecond>
			</li>
		</comps>
		<building>
			<isMealSource>true</isMealSource>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<heatPerTickWhileWorking>0.10</heatPerTickWhileWorking>
		</building>
	</ThingDef>

<!-- Mine shaft from medieval overhaul -->
	<ThingDef ParentName="BuildingBase">
		<defName>ISA_MineShaft</defName>
		<label>mine shaft</label>
		<description>A sturdy platform equipped with a primitive elevator built on top of a vertical tunnel. Can be used for mining resources and chunks.</description>
		<thingClass>Building_WorkTable</thingClass>
		<graphicData>
			<texPath>Production/MineShaft/MineShaft</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3.8,3.8)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<costList>
			<Steel>60</Steel>    
		</costList>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>300</costStuffCount>
		<designationCategory>Production</designationCategory>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.5</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<WorkToBuild>6000</WorkToBuild>
			<MaxHitPoints>500</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<Cleanliness>-15</Cleanliness>
		</statBases>
		<rotatable>false</rotatable>
		<size>(3,3)</size>
		<comps>
			<li Class="CompProperties_CreatesInfestations"/>
		</comps>
		<passability>Standable</passability>
		<pathCost>50</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,0)</interactionCellOffset>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<surfaceType>Item</surfaceType>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
		</building>
		<designationHotKey>Misc3</designationHotKey>
		<placeWorkers>
			<!--li>PlaceWorker_ShowFacilitiesConnections</li-->
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
		<researchPrerequisites>
			<li>Smithing</li>
		</researchPrerequisites>
		<techLevel>Medieval</techLevel>
	</ThingDef>

  <!-- Gardening box from medieval overhaul -->

	<ThingDef ParentName="FurnitureWithQualityBase">
		<defName>ISA_GardeningBox</defName>
		<label>gardening box</label>
		<description>A box filled with dirt to grow plants.</description>
		<thingClass>Building_PlantGrower</thingClass>
		<techLevel>Medieval</techLevel>
		<graphicData>
			<texPath>Production/GardeningBox/GardeningBox</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(1,4)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<statBases>
			<MaxHitPoints>80</MaxHitPoints>
			<WorkToBuild>300</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Cleanliness>-5</Cleanliness>
			<Mass>10</Mass>
		</statBases>
		<altitudeLayer>LowPlant</altitudeLayer>
		<size>(1,4)</size>
		<passability>PassThroughOnly</passability>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>70</costStuffCount>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<fertility>0.8</fertility>
		<designationCategory>Production</designationCategory>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<building>
			<defaultPlantToGrow>Plant_Rice</defaultPlantToGrow>
			<sowTag>Ground</sowTag>
			<paintable>false</paintable>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
	</ThingDef>

	<!-- ===== Presser ===== -->
	<ThingDef ParentName="BuildingBase">
		<defName>ISA_Presser</defName>
		<label>cheese press</label>
		<description>A wooden contraption to press selected items. Either to bring the juice out or whey from curds.</description>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<drawSize>(4,3)</drawSize>
			<texPath>Production/CheesePress/CheesePress</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<shadowData>
				<volume>(0.75, 0.35, 0.33)</volume>
			</shadowData>
		</graphicData>
		<neverMultiSelect>true</neverMultiSelect>
		<fillPercent>0.45</fillPercent>
		<pathCost>60</pathCost>
		<statBases>
			<WorkToBuild>600</WorkToBuild>
			<Mass>10</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<costList>
			<Steel>10</Steel>
		</costList>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>30</costStuffCount>
		<hasInteractionCell>False</hasInteractionCell>
		<!-- <interactionCellOffset>(0,0,-1)</interactionCellOffset> -->
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<building>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
		<size>(2,1)</size>
		<thingClass>Building_WorkTable</thingClass>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
			<li Class="ProcessorFramework.CompProperties_Processor">
				<capacity>100</capacity>
				<independentProcesses>false</independentProcesses>
				<parallelProcesses>false</parallelProcesses>
				<dropIngredients>true</dropIngredients>
				<showProductIcon>true</showProductIcon>
				<barOffset>(0.0, 0.25)</barOffset>
				<barScale>(1.0, 1.0)</barScale>
				<colorCoded>false</colorCoded>
				<processes>
					<li>ISA_PressCheeseProcess</li>
					<li MayRequire="Van.Beasts">ISA_MufftonMilkIntoCheese</li>
					<li MayRequire="Van.Beasts">ISA_GoldiloxMilkIntoCheese</li>
					<li MayRequire="OskarPotocki.VFE.Medieval2">ISA_PressGrapeProcess</li>
				</processes>
			</li>
		</comps>
		<tickerType>Rare</tickerType>
		<designationCategory>Production</designationCategory>
<!--		<researchPrerequisites>
			<li>DankPyon_Presser</li>
		</researchPrerequisites> -->
	</ThingDef>
	
</Defs>
