<?xml version="1.0" encoding="UTF-8"?>
<Defs>

<!-- Usa <minifiedDef>MinifiedThing</minifiedDef> per rendere le cose minifiable -->
  
<!-- Researching spot from medieval overhaul -->
  
	<ThingDef ParentName="BuildingBase">
		<defName>ISA_ResearchingSpot</defName>
		<label>researching spot</label>
		<description>A place for research simple concepts. Researching here is slow because of the lack of tools.</description>
		<thingClass>Building_ResearchBench</thingClass>
		<graphicData>
			<texPath>Things/Building/Production/CraftingSpot</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1,1)</drawSize>
		</graphicData>
		<altitudeLayer>FloorEmplacement</altitudeLayer>
		<statBases>
			<WorkToBuild>0</WorkToBuild>
			<ResearchSpeedFactor>0.5</ResearchSpeedFactor>
		</statBases>
		<scatterableOnMapGen>false</scatterableOnMapGen>
		<useHitPoints>false</useHitPoints>
		<size>(1,1)</size>
		<designationCategory>Production</designationCategory>
		<passability>Standable</passability>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<sowTag>SupportPlantsOnly</sowTag>
			<canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
			<ai_chillDestination>false</ai_chillDestination>
			<artificialForMeditationPurposes>false</artificialForMeditationPurposes>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
		<placeWorkers>
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
	</ThingDef>

<!-- Butcher's block from medieval overhaul -->

	  <ThingDef ParentName="BenchBase">
		<defName>ISA_ButchersBlock</defName>
		<label>butcher's block</label>
		<description>A wooden stump for butchering dead creatures into pieces of raw meat.</description>
  		<thingClass>Building_WorkTable</thingClass>
		<graphicData>
			<texPath>Building/Production/ButcherBlock/ButcherBlock</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(1.5,1.5)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<costStuffCount>50</costStuffCount>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.2</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>2000</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <Cleanliness>-15</Cleanliness>
    </statBases>
    <size>(1,1)</size>
    <designationCategory>Production</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <!--inherit recipes for mod compatibility-->
  		<modExtensions>
			<li Class="VEF.Buildings.RecipeInheritanceExtension">
				<inheritRecipesFrom>
					<li>TableButcher</li>
				</inheritRecipesFrom>
     			</li>
		</modExtensions>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
      <workTableRoomRole />
    </building>
    <comps>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>ToolCabinet</li>
        </linkableFacilities>
      </li>
    </comps>
    <designationHotKey>Misc3</designationHotKey>
    <placeWorkers>
      <li>PlaceWorker_ShowFacilitiesConnections</li>
      <li>PlaceWorker_PreventInteractionSpotOverlap</li>
    </placeWorkers>
		<techLevel>Medieval</techLevel>
  </ThingDef>

<!-- Rustic cooking table from medieval overhaul -->
  
	<ThingDef ParentName="ISA_1X3WorkbenchBase">
		<defName>ISA_RusticCookingTable</defName>
		<label>rustic cooking table</label>
		<description>A wood-fueled oven with an attached countertop for preparing meals.</description>
		<thingClass>Building_WorkTable_HeatPush</thingClass>
		<graphicData>
			<texPath>Building/Production/RusticCookingTable/RusticCookingTable</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3.5,1.5)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<castEdgeShadows>true</castEdgeShadows>
		<staticSunShadowHeight>0.20</staticSunShadowHeight>
		<costList>
			<WoodLog>60</WoodLog>
		</costList>
		<costStuffCount>20</costStuffCount>
		<stuffCategories>
			<li>Stony</li>
		</stuffCategories>
    <!--inherit recipes for mod compatibility-->
  		<modExtensions>
			<li Class="VEF.Buildings.RecipeInheritanceExtension">
				<inheritRecipesFrom>
					<li>FueledStove</li>
					<li>ElectricStove</li>
				</inheritRecipesFrom>
     			</li>
		</modExtensions>
		<comps>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>160.0</fuelConsumptionRate>
				<fuelCapacity>50.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>WoodLog</li>
						<li>ISA_Coal</li>
					</thingDefs>
				</fuelFilter>
				<consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
			</li>
			<li Class="CompProperties_HeatPusher">
				<compClass>CompHeatPusherPowered</compClass>
				<heatPerSecond>4</heatPerSecond>
			</li>
		</comps>
		<building>
			<isMealSource>true</isMealSource>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<heatPerTickWhileWorking>0.10</heatPerTickWhileWorking>
		</building>
	</ThingDef>

<!-- Mine shaft from medieval overhaul -->
	<ThingDef ParentName="BuildingBase">
		<defName>ISA_MineShaft</defName>
		<label>mine shaft</label>
		<description>A sturdy platform equipped with a primitive elevator built on top of a vertical tunnel. Can be used for mining resources and chunks.</description>
		<thingClass>Building_WorkTable</thingClass>
		<graphicData>
			<texPath>Building/Production/MineShaft/MineShaft</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3.8,3.8)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<costList>
			<Steel>60</Steel>    
		</costList>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>300</costStuffCount>
		<designationCategory>Production</designationCategory>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.5</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<WorkToBuild>6000</WorkToBuild>
			<MaxHitPoints>500</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<Cleanliness>-15</Cleanliness>
		</statBases>
		<rotatable>false</rotatable>
		<size>(3,3)</size>
		<comps>
			<li Class="CompProperties_CreatesInfestations"/>
		</comps>
		<passability>Standable</passability>
		<pathCost>50</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,0)</interactionCellOffset>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<surfaceType>Item</surfaceType>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
		</building>
		<designationHotKey>Misc3</designationHotKey>
		<placeWorkers>
			<!--li>PlaceWorker_ShowFacilitiesConnections</li-->
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
		<researchPrerequisites>
			<li>ISA_MiningResearch</li>
		</researchPrerequisites>
		<techLevel>Medieval</techLevel>
	</ThingDef>

  <!-- Gardening box from medieval overhaul -->

	<ThingDef ParentName="FurnitureWithQualityBase">
		<defName>ISA_GardeningBox</defName>
		<label>gardening box</label>
		<description>A box filled with dirt to grow plants.</description>
		<thingClass>Building_PlantGrower</thingClass>
		<techLevel>Medieval</techLevel>
		<graphicData>
			<texPath>Building/Production/GardeningBox/GardeningBox</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(1,4)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<statBases>
			<MaxHitPoints>80</MaxHitPoints>
			<WorkToBuild>300</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Cleanliness>-5</Cleanliness>
			<Mass>10</Mass>
		</statBases>
		<altitudeLayer>LowPlant</altitudeLayer>
		<size>(1,4)</size>
		<passability>PassThroughOnly</passability>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>70</costStuffCount>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<fertility>0.8</fertility>
		<designationCategory>Production</designationCategory>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<building>
			<defaultPlantToGrow>Plant_Rice</defaultPlantToGrow>
			<sowTag>Ground</sowTag>
			<paintable>false</paintable>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
		<researchPrerequisites>
			<li>ISA_Agriculture</li>
		</researchPrerequisites>
	</ThingDef>

<!-- Oven from medieval overhaul -->

	<ThingDef ParentName="ISA_1X3WorkbenchBase">
		<defName>ISA_StoneOven</defName>
		<label>stone oven</label>
		<thingClass>Building_WorkTable_HeatPush</thingClass>
		<description>A stone oven designed for baking meals.</description>
		<graphicData>
			<texPath>Building/Production/StoneOven/StoneOven</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(4,4)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<uiIconScale>0.70</uiIconScale>
		<size>(2,2)</size>
		<stuffCategories>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>120</costStuffCount>
		<statBases>
			<WorkToBuild>3000</WorkToBuild>
			<MaxHitPoints>200</MaxHitPoints>
			<Flammability>0</Flammability>
		</statBases>
		<drawerType>RealtimeOnly</drawerType> <!-- Keep for no fuel graphic overlay-->
		<comps>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>80.0</fuelConsumptionRate>
				<fuelCapacity>80.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>WoodLog</li>
						<li>ISA_Coal</li>
					</thingDefs>
				</fuelFilter>
				<consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
			</li>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
    	<recipes>
			<li>ISA_MakeBread</li>
			<li>ISA_MakeBerryPie</li>
			<li>ISA_MakeApplePie</li>
			<li>ISA_MakePumpkinPie</li>
			<li>ISA_MakeCarrotCake</li>
    	</recipes>
		<building>
			<isMealSource>true</isMealSource>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
		</building>
		<researchPrerequisites>
			<li>ISA_AdvancedCookingResearch</li>
		</researchPrerequisites>
	</ThingDef>

<!-- Alchemy table -->
  
	<ThingDef ParentName="ISA_1X3WorkbenchBase">
		<defName>ISA_AlchemyTable</defName>
		<label>alchemy table</label>
		<description>A heavy table, secured with a layer of thick leather. Stocked with all sorts of thingamajigs, it’s useless to uneducated mind whilst perfectly functional for an adept of forbidden arts. It’s everything an aspiring alchemist could wish for to mix ingredients and prepare oddly specific reagents.</description>
		<thingClass>Building_WorkTable</thingClass>
		<graphicData>
			<texPath>Building/Production/AlchemyTable/AlchemyTable</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3.5,1.5)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<castEdgeShadows>true</castEdgeShadows>
		<staticSunShadowHeight>0.20</staticSunShadowHeight>
		<costList>
			<WoodLog>60</WoodLog>
		</costList>
		<costStuffCount>20</costStuffCount>
		<stuffCategories>
			<li>Leathery</li>
		</stuffCategories>
    <!--inherit recipes for mod compatibility-->
  		<modExtensions>
			<li Class="VEF.Buildings.RecipeInheritanceExtension">
				<inheritRecipesFrom>
					<li>DrugLab</li>
				</inheritRecipesFrom>
     		</li>
		</modExtensions>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<paintable>true</paintable>
			<workTableRoomRole>Laboratory</workTableRoomRole>
			<workTableNotInRoomRoleFactor>0.8</workTableNotInRoomRoleFactor>
		</building>
