<?xml version="1.0" encoding="utf-8"?>
<Patch>

<Operation Class="PatchOperationSequence">
 <!-- <success>Always</success>  -->
    <operations>
       <!-- Add new caravan -->      <!-- Thank you Criz#2714 for the help with this patch -->

      <li Class="PatchOperationAdd">
        <xpath>/Defs/FactionDef[@Name="VFEM2_VikingBase"]/caravanTraderKinds</xpath>
        <value>
          <li MayRequire="ISA.RandomPatches">ISA_Caravan_Minerals</li>
          <li>ISA_Caravan_Mythological</li>
        </value>
      </li>

      <li Class="PatchOperationAdd">
        <xpath>/Defs/FactionDef[@Name="VFEM2_MedievalFactionBase"]/caravanTraderKinds</xpath>
        <value>
          <li MayRequire="ISA.RandomPatches">ISA_Caravan_Minerals</li>
          <li>ISA_Caravan_Mythological</li>
        </value>
      </li>

  </operations>
</Operation>

</Patch>
